create database simulaattori;
use simulaattori;

create table Ilmoittautuminen (
	id INTEGER not null AUTO_INCREMENT,
	asiakkaatLkm DOUBLE,
	jonoLopussa DOUBLE,
	kotiin DOUBLE,
	palvellut DOUBLE,
	kayttoaste DOUBLE,
	suoritusteho DOUBLE,
	palveluaika DOUBLE,
	oleskeluaika DOUBLE,
	lapimenoaika DOUBLE,
	jonoKeskim DOUBLE,
	saaNopeus DOUBLE,
	ilmNopeus DOUBLE,
	arvNopeus DOUBLE,
	laaNopeus DOUBLE,
	rtglabNopeus DOUBLE,
	PRIMARY KEY (id)
	);

